<div class="container" style="height: 800px;">
  <h2 style="padding-top: 120px;">Log in</h2>

  <p>Admin(email: "admin@ex.com", password: "oooooo")</p>

  <%= form_for(resource, as: resource_name, url: admins_sign_in_path) do |f| %>
    <div class="field">
      <%= f.label :email %><br />
      <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
    </div>

    <div class="field">
      <%= f.label :password %><br />
      <%= f.password_field :password, autocomplete: "current-password",:style=>"font-family: sans-serif;" %>
    </div>

    <% if devise_mapping.rememberable? %>
      <div class="field">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
      </div>
    <% end %>

    <div class="actions">
      <%= f.submit "Log in" %>
    </div>
  <% end %>
</div>